{
    "name": "Translator Extension",
    "description": "Build an Translator Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html"
    },
    "icon": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content-scripts.js"]
      }
    ],
    "host_permissions": [
         "https://translate.google.com/"
       ]
  }